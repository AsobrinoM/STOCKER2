<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/appbar"
        layout="@layout/toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <EditText
        android:id="@+id/ETPWBEmp"
        android:layout_width="219dp"
        android:layout_height="57dp"
        android:layout_marginEnd="108dp"
        android:ems="10"
        android:hint="@string/Web"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ETTLF" />

    <EditText
        android:id="@+id/ETTLF"
        android:layout_width="219dp"
        android:layout_height="57dp"
        android:layout_marginEnd="108dp"
        android:ems="10"
        android:hint="@string/correo_electr_nico"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ETCiuEmpr" />

    <EditText
        android:id="@+id/ETID"
        android:layout_width="219dp"
        android:layout_height="57dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="108dp"
        android:ems="10"
        android:hint="@string/txtCont"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ETNomEmpr" />

    <EditText
        android:id="@+id/ETCiuEmpr"
        android:layout_width="219dp"
        android:layout_height="57dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="108dp"
        android:ems="10"
        android:hint="@string/txtLoc"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ETID" />

    <EditText
        android:id="@+id/ETNomEmpr"
        android:layout_width="219dp"
        android:layout_height="57dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="108dp"
        android:ems="10"
        android:hint="@string/txtNomb"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbIntDat" />

    <TextView
        android:id="@+id/lbIntDat"
        android:layout_width="307dp"
        android:layout_height="27dp"

        android:layout_marginTop="136dp"
        android:text="@string/LbIntReg"
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.426"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnRegReg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnRegEmp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ETPWBEmp"
        app:layout_constraintVertical_bias="0.22" />
</androidx.constraintlayout.widget.ConstraintLayout>